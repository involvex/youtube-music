# Development Environment Variables for Bun Development Server

# Server Configuration
NODE_ENV=development
BUN_ENV=development
DEV_SERVER_PORT=3000
HMR_PORT=3001
HMR_HOST=localhost

# API Configuration
API_BASE_URL=https://music.youtube.com
WS_URL=ws://localhost:3001

# Feature Flags
ENABLE_HMR=true
ENABLE_PROXY=true
ENABLE_CORS=true

# Build Configuration
SOURCE_MAP=true
MINIFY=false
SPLITTING=false

# Development Tools
ENABLE_DEBUG=true
ENABLE_LOGGING=true
ENABLE_INSPECTOR=false

# Performance
WATCH_MODE=true
HOT_RELOAD=true
LIVE_RELOAD=true

# Security
CSP_ENABLED=false
HTTPS_ENABLED=false